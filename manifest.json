{
  "manifest_version": 2,

  "name": "Easy TQFR",
  "description": "This extension runs Python code.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
  "scripts": [ "js/jquery-3.3.1.min.js", "js/popper.min.js", "js/bootstrap.min.js"],
  "persistent": false
  },
  "options_page": "options.html",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "https://access.caltech.edu/*",
    "storage"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["js/content_script.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    }
  ]
}
